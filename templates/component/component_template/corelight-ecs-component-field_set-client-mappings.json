{
  "_meta": {
    "description": "Corelight ECS Templates. For technical issues, questions, and documentation see https://github.com/corelight/ecs-templates"
  },
  "version": 2024011501,
  "template": {
    "settings": {
      "index": {
        "analysis": {
          "analyzer": {
            "es_stk_analyzer": {
              "char_filter": [
                "whitespace_no_way"
              ],
              "filter": [
                "trim",
                "lowercase",
                "max_token_truncate"
              ],
              "tokenizer": "keyword",
              "type": "custom"
            }
          },
          "char_filter": {
            "whitespace_no_way": {
              "pattern": "(\\s)+",
              "replacement": "$1",
              "type": "pattern_replace"
            }
          },
          "filter": {
            "max_token_truncate": {
              "length": "32766",
              "type": "truncate"
            },
            "path_hierarchy_pattern_filter": {
              "patterns": [
                "((?:[^\\\\]*\\\\)*)(.*)",
                "((?:[^/]*/)*)(.*)"
              ],
              "preserve_original": "true",
              "type": "pattern_capture"
            }
          },
          "tokenizer": {
            "path_tokenizer": {
              "delimiter": "\\",
              "type": "path_hierarchy"
            }
          }
        }
      }
    },
    "mappings": {
      "properties": {
        "client": {
          "properties": {
            "address": {
              "ignore_above": 32765,
              "type": "keyword"
            },
            "as": {
              "properties": {
                "number": {
                  "type": "long"
                },
                "organization": {
                  "properties": {
                    "name": {
                      "fields": {
                        "security": {
                          "type": "text",
                          "analyzer": "es_stk_analyzer"
                        }
                      },
                      "ignore_above": 32765,
                      "type": "keyword"
                    }
                  }
                }
              }
            },
            "bytes": {
              "type": "long"
            },
            "domain": {
              "ignore_above": 32765,
              "type": "keyword",
              "fields": {
                "security": {
                  "type": "text",
                  "analyzer": "es_stk_analyzer"
                }
              }
            },
            "geo": {
              "properties": {
                "city_name": {
                  "ignore_above": 32765,
                  "type": "keyword",
                  "fields": {
                    "security": {
                      "type": "text",
                      "analyzer": "es_stk_analyzer"
                    }
                  }
                },
                "continent_code": {
                  "ignore_above": 32765,
                  "type": "keyword"
                },
                "continent_name": {
                  "ignore_above": 32765,
                  "type": "keyword",
                  "fields": {
                    "security": {
                      "type": "text",
                      "analyzer": "es_stk_analyzer"
                    }
                  }
                },
                "country_iso_code": {
                  "ignore_above": 32765,
