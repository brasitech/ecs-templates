                },
                "country_name": {
                  "ignore_above": 32765,
                  "type": "keyword",
                  "fields": {
                    "security": {
                      "type": "text",
                      "analyzer": "es_stk_analyzer"
                    }
                  }
                },
                "location": {
                  "type": "geo_point"
                },
                "name": {
                  "ignore_above": 32765,
                  "type": "keyword",
                  "fields": {
                    "security": {
                      "type": "text",
                      "analyzer": "es_stk_analyzer"
                    }
                  }
                },
                "postal_code": {
                  "ignore_above": 32765,
                  "type": "keyword"
                },
                "region_iso_code": {
                  "ignore_above": 32765,
